{
  "name": "@ladder-ui/utils",
  "version": "0.1.4",
  "description": "ladder UI Utils",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib/**",
    "LICENSE",
    "README.md",
    "*.d.ts",
    "*.css",
    "*.js",
    "*.mjs"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"

    "test:coverage": "echo \"Should be set\"",
    "lint": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --cache --max-warnings=0",
    "lint:fix": "eslint *.ts{,x} --fix"
  },
  "keywords": [
    "react",
    "typescript",
    "react-library",
    "button"
  ],
  "author": "Ivan Avila (ivelavalm)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivelaval/ladder-ui"
  },
  "bugs": {
    "url": "https://github.com/ivelaval/ladder-ui/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.1",
    "typescript": "^5.0.3"
  }
